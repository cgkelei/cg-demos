<Pipeline>	<Setup>		<FrameBuffer name="GBuffer" width="1024" height="1024">			<Target type="Depth" attach="Depth" format="Depth32Stencil8"/>			<Target type="Color" attach="Color0" format="A16B16G16R16F"/>			<Target type="Color" attach="Color1" format="A16B16G16R16F"/>			<Target type="Color" attach="Color2" format="A16B16G16R16F"/>		</FrameBuffer>	</Setup>		<CommandQueue>		<Stage name="Attrib">			<SwitchFrameBuffer name="GBuffer"/>			<Clear attach="Depth" depth="1.0" stencil="0.0"/>			<Clear attach="Color0" r="0.0" g="0.0" b="0.0" a="0.0"/>			<Clear attach="Color1" r="0.0" g="0.0" b="0.0" a="0.0"/>			<Clear attach="Color2" r="0.0" g="0.0" b="0.0" a="0.0"/>							<DrawGeometry />		</Stage>				<Stage name="Lighting">			<SwitchFrameBuffer name="Screen"/>			<Clear attach="Depth" depth="1.0" stencil="0.0"/>			<Clear attach="Color0" r="0.0" g="0.0" b="0.0" a="0.0"/>			</CommandQueue></Pipeline>